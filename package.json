{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "yabai-extended",
    "title": "Yabai Extended",
    "description": "Enhance Yabai extension",
    "icon": "yabai-icon-512px.png",
    "author": "razvanalex",
    "contributors": [ ],
    "categories": [
        "Developer Tools",
        "Productivity"
    ],
    "license": "MIT",
    "commands": [
        {
            "name": "move-window-space-prev",
            "title": "Move Window to Previous Space",
            "description": "Move the current window to the previous space",
            "mode": "no-view"
        }
    ],
    "preferences": [
        {
            "name": "yabaiPath",
            "title": "Yabai installation path",
            "required": false,
            "type": "textfield",
            "description": "Location to the Yabai installation (Defaults to `/opt/homebrew/bin/yabai` on M1 Macs, and `/usr/local/bin/yabai` otherwise)"
        },
        {
            "name": "resizeStep",
            "type": "number",
            "required": false,
            "default": 50,
            "title": "Resize Step",
            "description": "The number of pixels to resize windows by (default: 50)"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.64.4",
        "@raycast/utils": "^1.10.1",
        "execa": "^8.0.1",
        "ray": "^0.0.1",
        "tiny-pinyin": "^1.3.2"
    },
    "devDependencies": {
        "@raycast/eslint-config": "1.0.8",
        "@types/node": "20.10.5",
        "@types/react": "18.2.45",
        "eslint": "^8.56.0",
        "prettier": "^3.1.1",
        "typescript": "^5.3.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint",
        "publish": "npx @raycast/api@latest publish"
    }
}